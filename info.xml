<?xml version="1.0"?>
<extension key="civicall" type="module">
  <file>civicall</file>
  <name>CiviCall</name>
  <description>Callcenter extension for CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/civicall</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/civicall</url>
    <url desc="Support">https://github.com/greenpeace-cee/civicall/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-03-05</releaseDate>
  <version>1.0.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.57</ver>
  </compatibility>
  <requires>
    <ext>org.civicrm.search_kit</ext>
    <ext>org.civicrm.afform</ext>
  </requires>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Civicall</namespace>
    <format>23.02.1</format>
    <angularModule>crmCivicall</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Civicall_Upgrader</upgrader>
</extension>
